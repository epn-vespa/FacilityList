<script>
jQuery('.page-title').html(function(_, existing) {
  var words = existing.trim().split(' ');
  words[0] = '<span class="title-firstword">' + words[0] + '</span>';
  words[words.length - 1] = '<span class="title-lastword">' + words[words.length - 1] + '</span>';
  return words.join(' ');
});
</script>

